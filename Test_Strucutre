	<script type="text/javascript" src="js/jquery-1.12.3.js"></script>
	<script type="text/javascript" src="js/bootstrap.min.js"></script>
	<script type="text/javascript">
		$(document).ready(function(){
			var containerWidth = $(".grid_content_wrapper").css("width");
			console.log(containerWidth);
			var leftPaneWidth = $(".left_header").css("width");
			console.log(leftPaneWidth);
			var rightPaneWidth = $(".right_header").css("width");
			console.log(rightPaneWidth);
			
			var centerBodyWidth = parseInt(containerWidth) - parseInt(leftPaneWidth) - parseInt(rightPaneWidth);
			console.log(centerBodyWidth);
			$(".center_header").css("width", centerBodyWidth);
			
			var node_value = 0;
			
			$("div.filter_icon").click(function(e){
				var attribute = $(this).attr("data-node");
				if(node_value != attribute)
				{
					if($(".grid_filter_menu").css("display")=="block")
					{
						console.log("X	"+e.pageX+"	Y	"+e.pageY);
						$(".grid_filter_menu").hide();
						$(".grid_filter_menu").show(500);
						$(".grid_filter_menu").offset({left: e.pageX-150, top: e.pageY+15});
					}
					else
					{
						console.log("X	"+e.pageX+"	Y	"+e.pageY);
						$(".grid_filter_menu").show(500);
						$(".grid_filter_menu").offset({left: e.pageX-150, top: e.pageY+15});
					}
					node_value = attribute;
				}
				else
				{
					if($(".grid_filter_menu").css("display")=="block")
					{
						$(".grid_filter_menu").hide(500);
					}
					else
					{
						console.log("X	"+e.pageX+"	Y	"+e.pageY);
						$(".grid_filter_menu").show(500);
						$(".grid_filter_menu").offset({left: e.pageX-150, top: e.pageY+15});
					}
				}
			});
			
/* 			$(document).mouseup(function(event)
			{
				if(!$(".grid_filter_menu").is(event.target) && $(".grid_filter_menu").has(event.target).length===0)
				{
					if($(".grid_filter_menu").css("display")=="block")
					{
						$(".grid_filter_menu").hide(500);
					}
				}
			}); */
		});
	</script>
	
	<script>
		(function(){
			var target_left = $(".grid_left_body");
			var target_center = $(".grid_center_body");
			$(".grid_right_body").scroll(function(){
				target_left.prop("scrollTop", this.scrollTop);
				target_center.prop("scrollTop", this.scrollTop);
			});
		})();
				
		(function(){
			var target = $(".grid_header_viewport.center_header");
			$(".grid_center_body").scroll(function(){
				target.prop("scrollLeft", this.scrollLeft);
			});
		})();
		
		$(".grid_center_body").mouseover(function(event){
			console.log("STEP1");
			event.preventDefault();
			$(".grid_center_body").scroll(function(event){
				console.log("STEP2");
				scrollGrid(event);
			});
		});
		$(".grid_left_body").mouseover(function(event){
			console.log("STEP3");
			event.preventDefault();
			$(".grid_left_body").scroll(function(event){
				console.log("STEP4");
				scrollGrid(event);
			});
		});
		
		function scrollGrid(event)
		{
			console.log("STEP5");
			event.preventDefault();
			var target_left = $(".grid_left_body");
			var target_center = $(".grid_center_body");
			$(".grid_right_body").scroll(function(){
				target_left.prop("scrollTop", this.scrollTop);
				target_center.prop("scrollTop", this.scrollTop);
			});
		}
		
		/* (function(){
			var target_center = $(".grid_center_body");
			var target_right = $(".grid_right_body");
			$(".grid_left_body").scroll(function(){
				target_right.prop("scrollTop", this.scrollTop);
				target_center.prop("scrollTop", this.scrollTop);
			});
		})();
		(function(){
			var target_left = $(".grid_left_body");
			var target_right = $(".grid_right_body");
			$(".grid_center_body").scroll(function(){
				target_left.prop("scrollTop", this.scrollTop);
				target_right.prop("scrollTop", this.scrollTop);
			});
		})(); */
	</script>
	
</body>
</html>






















