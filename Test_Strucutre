.table_block
{
	width: 94%;
	height: 400px;
	margin-left: 3%;
	padding: 10px 5px;
	margin-top: 10px;
	background-color: #e1e1e1;
	border-radius: 3px;
}

.grid_scope
{
	width: 100%;
	height: 340px;
}

.grid_panel
{
	border: 1px solid #d4d4d4;
	box-sizing: content-box;
	border-radius: 0px;
	transform: translateZ(0);
